{
  "name": "whatsapp-clone",
  "version": "1.0.0",
  "description": "Whatsapp Clone",
  "main": "index.js",
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.tsx": [
      "tslint",
      "npm run format",
      "git add"
    ]
  },
  "scripts": {
    "build": "webpack --env.mode production -p",
    "dev:client": "webpack-dev-server --env.mode development",
    "dev:server": "cd server && npm run dev",
    "dev": "npm-run-all --parallel dev:*",
    "prestart": "npm run build && npm run codegen",
    "start:client": "serve -s dist",
    "start:server": "cd server && npm start",
    "start": "npm-run-all --parallel start:*",
    "codegen": "cd server && npm run codegen && cd .. && gql-gen",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seb7887/whatsapp-clone.git"
  },
  "author": "Sebastian Matias Segura (@seb7887)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seb7887/whatsapp-clone/issues"
  },
  "homepage": "https://github.com/seb7887/whatsapp-clone#readme",
  "devDependencies": {
    "@graphql-codegen/add": "^1.4.0",
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/typescript": "^1.4.0",
    "@graphql-codegen/typescript-operations": "^1.4.0",
    "@graphql-codegen/typescript-react-apollo": "^1.4.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.4",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@welldone-software/why-did-you-render": "^3.2.1",
    "apollo-link-mock": "^1.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "fast-sass-loader": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-watch-select-projects": "^0.1.2",
    "lint-staged": "^9.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
